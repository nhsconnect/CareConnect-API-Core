			<profile>
				<reference value="https://fhir.hl7.org.uk/STU3/StructureDefinition/CareConnect-Observation-1"/>
			</profile>
			<interaction>
				<extension url="http://hl7.org/fhir/StructureDefinition/capabilitystatement-expectation">
					<valueCode value="SHALL"/>
				</extension>
				<code value="read"/>
			</interaction>
			<interaction>
				<extension url="http://hl7.org/fhir/StructureDefinition/capabilitystatement-expectation">
					<valueCode value="SHALL"/>
